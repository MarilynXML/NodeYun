<% var title = '云-视频',
	styles=['/public/styles/css/videos.css','/public/styles/css/video-js.css']
%>

<% include head %>
<% include templates/header %>
<% include templates/side %>
<div class="container-fluid">
	<input type="hidden" id="cloud-url" value="<%= cloudUrl %>">
	<% include templates/banner %>
	<% include templates/upload_modal %>
	<ul class="files">
		<% include templates/file_banner %>
		
		<% videos.forEach(function(file,index){ %>
			<% include templates/file_item %>
		<% });%>
	</ul>
	<div class="tip-completed">已经加载完</div>
	<div class="tip-share"></div>
	<% include templates/video %>
</div>
<div class="video-overlay"></div>
<% var main='videos' %>
<% include foot %>